{
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "func-names": "off",
    "eqeqeq": "off",
    "no-param-reassign": "off",
    "object-curly-newline": "off",
    "consistent-return": "off",
    "linebreak-style": 0,
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ]
  }
}